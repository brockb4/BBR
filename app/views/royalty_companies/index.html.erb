<h1>Listing royalty_companies</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Royalty</th>
  </tr>

<% @royalty_companies.each do |company| %>
  <tr>
    <td><%= link_to company.name, edit_royalty_company_path(company) %></td>
    <td><%= company.royalty %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Royalty Company', new_royalty_company_path %>
